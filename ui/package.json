    "backbone": "^1.3.3",
    "bootstrap": "4.4.1",
    "core-js": "~3.6",
    "dagre": "~0.8.2",
    "file-saver": "~2.0.0",
    "graphlib": "~2.1.5",
    "jointjs": "^3",
    "jquery": "^3.4.1",
