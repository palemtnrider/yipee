{
  "name": "yipee",
  "version": "2.2.0",
  "peerDependencies": {
    "@angular/core": "~9.0.4",
    "@angular-devkit/core": "~9.0.4",
    "ajv": "^6.0.0"
  },
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "clean": "rm -rf ./dist",
    "clean-all": "rm -rf ./dist && rm -rf ./node_modules",
    "start": "ng serve --port 8080 --proxy-config proxy.config.js",
    "build": "ng build",
    "coverage": "ng test --code-coverage",
    "build-prod-version": "npm version patch && node ./replace.build.js && npm run build-prod",
    "build-prod": "npm run clean && ng build --prod",
    "test-prod": "echo 'set TEST_FILE to run a single test' && npm run clean && npm run lint && npm run test && ng build --prod",
    "build-beta": "npm run clean && npm run lint && npm run test && ng build --target=production --env=beta --base-href=/beta/",
    "test": "echo 'set TEST_FILE to run a single test' && ng test --watch=false --source-map=false",
    "test-watch": "echo 'set TEST_FILE to run a single test' && ng test --source-map=false",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.0.4",
